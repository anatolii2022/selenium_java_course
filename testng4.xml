<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="E-Commerce" parallel="tests" thread-count="2">
	<listeners>
		<listener
			class-name="com.udemy.selenium.testngtutorial.MyListeners"></listener>
	</listeners>

	<parameter name="URL" value="www.google.com" />

	<test name="Login">
		<parameter name="APIKey" value="3456800322245"></parameter>
		<classes>
			<class name="com.udemy.selenium.testngtutorial.Lesson002" />
		</classes>
	</test>

	<test name="Search">
		<parameter name="searchText" value="Asus ROG M16"></parameter>
		<classes>
			<class name="com.udemy.selenium.testngtutorial.Lesson0021" />
		</classes>
	</test>

</suite> <!-- Suite -->
